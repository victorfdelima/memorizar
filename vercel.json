{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/routes/index.js"
        },
        {
            "functions": {
                "routes/test.js": {
                    "memory": 3008,
                    "maxDuration": 30
                },
                "routes/*.js": {
                    "memory": 3008,
                    "maxDuration": 30
                }
            }
        }
    ]
}